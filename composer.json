{
    "name": "netinteractive/elegant",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/halfik/elegant",
    "keywords": ["laravel", "model", "blueprint", "mapper", "data mapper architecture", "elegant", "netinteractive", "laravel5"],
    "version": "3.0.0@dev",
    "description": "",
    "authors": [
        {
            "name": "Piotr Pryga",
            "email": "halfik@gmail.com",
            "role": "Owner"
        },
        {
            "name": "Michał Szapowałow",
            "email": "smikebox@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "netinteractive/http": ">=1.0.0",
        "illuminate/support": "5.0 - 5.2",
        "illuminate/pagination": "5.0 - 5.2",
        "illuminate/contracts": "5.0 - 5.2",
        "illuminate/console": "5.0 - 5.2",
        "illuminate/container": "5.0 - 5.2",
        "illuminate/filesystem": "5.0 - 5.2",
        "illuminate/events": "5.0 - 5.2",
        "nesbot/carbon": "~1.20",
        "opis/closure": ">=1.3.0"
    },
  "repositories": [
    {
      "type":"git",
      "url": "https://github.com/halfik/http"
    }
  ],
    "require-dev": {
      "codeception/codeception": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "files": [
          "src/Netinteractive/Elegant/helpers.php"
        ],
        "psr-0": {
            "Netinteractive\\Elegant\\": "src/"
        }
    },
    "suggest": {
      "doctrine/dbal": "Required to rename columns and drop SQLite columns (~2.4)."
    },
    "minimum-stability": "dev"
}
